# Database for PositionerPositionCompare functions in Newport XPS
# Mark Rivers
# March 10, 2015

record(bo,"$(P)$(R)PositionCompareEnable") {
    field(DTYP, "asynInt32")
    field(OUT,"@asyn($(PORT),$(ADDR))XPS_POSITION_COMPARE_ENABLE")
    field(ZNAM, "Disable")
    field(ONAM, "Enable")
}

record(bi,"$(P)$(R)PositionCompareEnable_RBV") {
    field(DTYP, "asynInt32")
    field(INP,"@asyn($(PORT),$(ADDR))XPS_POSITION_COMPARE_ENABLE")
    field(ZNAM, "Disable")
    field(ONAM, "Enable")
    field(SCAN, "I/O Intr")
}

